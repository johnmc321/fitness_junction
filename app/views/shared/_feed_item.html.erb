<li id="<%= feed_item.id %>">
  <%= link_to image_tag("map2.jpg", alt: "Rails", height: "70px", width: "70px"), '#' %>
  <span class="content"><%= feed_item.name %> <%= feed_item.activity_type %></span>
  <span class="user">
    by: <%= link_to feed_item.user.name, feed_item.user %>
  </span>

  

  
    <span class="timestamp">
    Added <%= time_ago_in_words(feed_item.created_at) %> ago.
  </span><p><%= link_to "view activity", "#" %> | 

  <% if current_user?(feed_item.user) %>
    <%= link_to "delete", feed_item, method: :delete,
                                     data: { confirm: "You sure?" },
                                     title: feed_item.name %></p>
  <% end %>
</li>